    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: flask",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "console": "integratedTerminal",
            "env": {
                "FLASK_APP":"flask-backend/app.py",
                "FLASK_ENV":"development",
                "FLASK_RUN_HOST":"0.0.0.0",
                "FLASK_RUN_PORT":"8080"
            },
            "args": [
                "run"
            ]
        }
    ]
}